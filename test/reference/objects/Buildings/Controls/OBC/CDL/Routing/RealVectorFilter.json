                  },
                  {
                    "element_modification_or_replaceable": {
                      "element_modification": {
                        "name": "info",
                        "modification": {
                          "equal": true,
                          "expression": {
                            "simple_expression": "\"<html>\n<p>\nThis block filters a Real vector of size <code>nin</code> to\na vector of size <code>nout</code> given a boolean mask\n<code>msk</code>.\n</p>\n<p>\nIf an entry in <code>msk</code> is <code>true</code>, then the value\nof this input will be sent to the output <code>y</code>, otherwise it\nwill be discarded.\n</p>\n<p>\nThe parameter <code>msk</code> must have exactly <code>nout</code> entries\nset to <code>true</code>, otherwise an error message is issued.\n</p>\n</html>\""
                          }
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        }
